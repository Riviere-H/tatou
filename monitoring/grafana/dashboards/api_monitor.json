{
  "id": null,
  "uid": "tatou_api_monitor",
  "title": "Tatou API Monitoring",
  "schemaVersion": 37,
  "version": 1,
  "time": {"from": "now-1h", "to": "now"},
  "refresh": "5s",
  "timezone": "browser",
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "API Request Rate",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
      "targets": [{"expr": "rate(http_requests_total[5m])", "refId": "A"}]
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "API Error Rate",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
      "targets": [{"expr": "sum by (endpoint, method) (api_errors_total)", "refId": "A"}]
    },
    {
      "id": 3,
      "type": "table",
      "title": "Endpoint Performance",
      "gridPos": {"h": 10, "w": 24, "x": 0, "y": 8},
      "targets": [{"expr": "sum by (endpoint, method) (api_errors_total)", "refId": "A", "format": "table", "instant": true}]
    }
  ]
}
